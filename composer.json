{
    "name": "styleci/sdk",
    "description": "The StyleCI PHP SDK",
    "keywords": ["api", "sdk", "styleci", "API", "SDK", "StyleCI", "StyleCI API", "StyleCI SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "StyleCI",
            "email": "support@styleci.io"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "graham-campbell/guzzle-factory": "^3.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "graham-campbell/testbench-core": "^1.1",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\SDK\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
