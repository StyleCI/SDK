{
    "name": "styleci/sdk",
    "description": "The StyleCI PHP SDK",
    "keywords": ["api", "sdk", "styleci", "API", "SDK", "StyleCI", "StyleCI API", "StyleCI SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "StyleCI",
            "email": "support@styleci.io"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "graham-campbell/guzzle-factory": "^4.2"
    },
    "require-dev": {
        "graham-campbell/testbench-core": "^3.2.2",
        "phpunit/phpunit": "^8.5.13 || ^9.4.4",
        "mockery/mockery": "~1.3.3 || ^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
