{
    "name": "styleci/sdk",
    "description": "The StyleCI PHP SDK",
    "keywords": ["sdk", "alt-three", "styleci", "SDK", "Alt Three", "StyleCI"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "graham-campbell/guzzle-factory": "^2.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "graham-campbell/testbench-core": "^1.1",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\SDK\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
